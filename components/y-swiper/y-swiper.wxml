<!--components/y-swiper/y-swiper.wxml-->
<swiper class="swiper"
        circular
        autoplay
        interval="3000"
        indicator-dots
        indicator-active-color="#ff5777">
  <block wx:for="{{list}}" wx:key="inedx">
    <swiper-item class="swiper-item">
      <image src="{{item.image || item}}" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>
